            { "name": "Insertion_Order_Status", "type": "STRING" },
            { "name": "Insertion_Order_Integration_Code", "type": "STRING" },
            { "name": "Line_Item", "type": "STRING" },
            { "name": "Line_Item_ID", "type": "INTEGER" },
